{
  "name": "wesplit",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "scripts": {
    "** Miscellaneous **": "",
    "prepare": "husky",
    "** App **": "",
    "ng": "ng",
    "start:server": "npx vercel dev",
    "start": "ng serve --host 0.0.0.0 --disable-host-check --ssl",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:unit": "ng test --include=src/utils/*.spec.ts",
    "*** Workflow ***": "",
    "lint": "npm run lint:code && npm run lint:format && npm run lint:styles && npm run lint:types",
    "lint:code": "ng lint",
    "lint:code:fix": "ng lint --fix",
    "lint:format": "prettier --check ./",
    "lint:format:fix": "prettier --write ./",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "lint:styles:fix": "stylelint \"src/**/*.scss\" --fix",
    "lint:types": "tsc --noEmit --strict --incremental false --tsBuildInfoFile null --project tsconfig.json"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.scss": [
      "stylelint --fix"
    ],
    "*.{js,ts,html,svg,yml,md,less,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@angular/animations": "18.2.8",
    "@angular/common": "18.2.8",
    "@angular/compiler": "18.2.8",
    "@angular/core": "18.2.8",
    "@angular/fire": "18.0.1",
    "@angular/forms": "18.2.8",
    "@angular/material": "18.2.8",
    "@angular/platform-browser": "18.2.8",
    "@angular/platform-browser-dynamic": "18.2.8",
    "@angular/router": "18.2.8",
    "@fontsource/roboto": "5.1.0",
    "@ngneat/transloco": "6.0.4",
    "@vercel/analytics": "1.3.1",
    "html5-qrcode": "2.3.8",
    "lint-staged": "15.4.3",
    "lodash-es": "4.17.21",
    "material-icons": "1.13.12",
    "moment": "2.30.1",
    "process": "0.11.10",
    "rxjs": "7.8.1",
    "tslib": "2.7.0",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.8",
    "@angular/cli": "18.2.8",
    "@angular/compiler-cli": "18.2.8",
    "@commitlint/cli": "19.7.1",
    "@commitlint/config-conventional": "19.7.1",
    "@ngx-env/builder": "18.0.2",
    "@types/jasmine": "5.1.4",
    "@types/node": "20.16.11",
    "@vercel/node": "3.2.23",
    "angular-eslint": "19.1.0",
    "eslint": "^9.20.0",
    "form-data": "4.0.1",
    "husky": "9.1.7",
    "jasmine-core": "5.2.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "node-fetch": "3.3.2",
    "prettier": "3.3.3",
    "sass": "1.79.5",
    "stylelint": "16.15.0",
    "stylelint-config-sass-guidelines": "12.1.0",
    "stylelint-order": "6.0.4",
    "stylelint-scss": "6.11.1",
    "typescript": "5.5.4",
    "typescript-eslint": "8.23.0"
  }
}
